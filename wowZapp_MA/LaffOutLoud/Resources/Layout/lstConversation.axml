<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:d1p1="http://schemas.android.com/apk/res/android"
    d1p1:orientation="horizontal"
    d1p1:minWidth="25px"
    d1p1:minHeight="25px"
    d1p1:layout_width="fill_parent"
    d1p1:layout_height="wrap_content"
    d1p1:id="@+id/linearLayout1"
    d1p1:gravity="top">
    <LinearLayout
        d1p1:orientation="horizontal"
        d1p1:minWidth="25px"
        d1p1:minHeight="25px"
        d1p1:layout_width="wrap_content"
        d1p1:layout_height="fill_parent"
        d1p1:id="@+id/linearLayout8">
        <ImageView
            d1p1:src="@drawable/defaultuserimage"
            d1p1:layout_width="56dp"
            d1p1:layout_height="56dp"
            d1p1:id="@+id/imgProfile1"
            d1p1:layout_gravity="center_vertical" />
        <LinearLayout
            d1p1:orientation="vertical"
            d1p1:minWidth="25px"
            d1p1:minHeight="25px"
            d1p1:layout_width="fill_parent"
            d1p1:layout_height="wrap_content"
            d1p1:id="@+id/linearLayout2">
            <LinearLayout
                d1p1:orientation="vertical"
                d1p1:minWidth="25px"
                d1p1:minHeight="25px"
                d1p1:layout_width="fill_parent"
                d1p1:layout_height="wrap_content"
                d1p1:id="@+id/linearLayout3">
                <TextView
                    d1p1:text="Text"
                    d1p1:layout_width="fill_parent"
                    d1p1:layout_height="wrap_content"
                    d1p1:id="@+id/textNames"
                    d1p1:textColor="#fff"
                    d1p1:textSize="10dp"
                    d1p1:layout_marginLeft="12dp"
                    d1p1:paddingTop="1dp" />
            </LinearLayout>
            <LinearLayout
                d1p1:orientation="vertical"
                d1p1:minWidth="25px"
                d1p1:minHeight="25px"
                d1p1:layout_width="fill_parent"
                d1p1:layout_height="wrap_content"
                d1p1:id="@+id/linearLayout5">
                <TextView
                    d1p1:layout_width="fill_parent"
                    d1p1:layout_height="30dp"
                    d1p1:id="@+id/textMessageBubble"
                    d1p1:background="@drawable/bubblesolidleft"
                    d1p1:textSize="12dp"
                    d1p1:textColor="#fff"
                    d1p1:gravity="top"
                    d1p1:paddingLeft="12dp"
                    d1p1:layout_marginRight="2dp" />
            </LinearLayout>
            <LinearLayout
                d1p1:orientation="horizontal"
                d1p1:minWidth="25px"
                d1p1:minHeight="25px"
                d1p1:layout_width="fill_parent"
                d1p1:layout_height="14dp"
                d1p1:id="@+id/linearLayout7"
                d1p1:background="@drawable/attachmentspreviewbkgr"
                d1p1:layout_marginLeft="8dp"
                d1p1:layout_marginRight="2dp" />
            <LinearLayout
                d1p1:orientation="horizontal"
                d1p1:minWidth="25px"
                d1p1:minHeight="25px"
                d1p1:layout_width="fill_parent"
                d1p1:layout_height="wrap_content"
                d1p1:id="@+id/linearLayout6"
                d1p1:gravity="left"
                d1p1:weightSum="1">
                <LinearLayout
                    d1p1:orientation="horizontal"
                    d1p1:minWidth="25px"
                    d1p1:minHeight="25px"
                    d1p1:layout_width="wrap_content"
                    d1p1:layout_height="fill_parent"
                    d1p1:id="@+id/linearLayout10"
                    d1p1:layout_weight=".5"
                    d1p1:layout_marginLeft="8dp"
                    d1p1:layout_marginRight="2dp">
                    <TextView
                        d1p1:text="@string/conversationUISent"
                        d1p1:layout_width="wrap_content"
                        d1p1:layout_height="fill_parent"
                        d1p1:id="@+id/textView1"
                        d1p1:layout_marginRight="6.0dp"
                        d1p1:textSize="12dp"
                        d1p1:textColor="#fff" />
                    <TextView
                        d1p1:layout_width="wrap_content"
                        d1p1:layout_height="fill_parent"
                        d1p1:id="@+id/textDateSent"
                        d1p1:textSize="12dp"
                        d1p1:textColor="#fff" />
                </LinearLayout>
                <LinearLayout
                    d1p1:orientation="horizontal"
                    d1p1:minWidth="25px"
                    d1p1:minHeight="25px"
                    d1p1:layout_width="wrap_content"
                    d1p1:layout_height="fill_parent"
                    d1p1:id="@+id/linearLayout9"
                    d1p1:layout_weight=".5"
                    d1p1:layout_marginRight="2dp"
                    d1p1:gravity="right">
                    <TextView
                        d1p1:text="@string/conversationUINoUnreadMessages"
                        d1p1:layout_width="wrap_content"
                        d1p1:layout_height="fill_parent"
                        d1p1:id="@+id/txtMessageUnread"
                        d1p1:textSize="12dp"
                        d1p1:textColor="#fff" />
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</LinearLayout>